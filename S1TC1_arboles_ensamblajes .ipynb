    "# Celda 7\n",
    "\n",
    "# Creación de las 300 muestras de Bootstrap\n",
    "np.random.seed(123)\n",
    "n_samples = X_train.shape[0] \n",
    "B = 300\n",
    "samples = [np.random.choice(a = n_samples, size = n_samples, replace = True) for _ in range(1,B+1)]\n",
    "samples"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Creación de los modelos diferentes por cada muestra de bootstrap\n",
    "\n",
    "## Crear dataframe para guardar las predicciones\n",
    "y_pred = pd.DataFrame(index = X_test.index, columns = [list(range(B))])\n",
    "\n",
    "## Entrenamiento de modelos para los tres grupos diferentes de modelos con las 300 muestras de Bootstrap\n",
    "model1 = DecisionTreeClassifier(max_depth = 40, random_state = 123)\n",
    "model2 = DecisionTreeClassifier(min_samples_leaf = 20, random_state = 123)\n",
    "model3 = LogisticRegression(random_state = 123, max_iter = 1000)\n",
    "\n",
    "for i,sample in enumerate(samples):\n",
    "\n",
    "    # Division en grupo de entrenamiento y testeo\n",
    "    Xtrain = X_train.iloc[sample]\n",
    "    ytrain = y_train.iloc[sample]\n",
    "    # X_Test = X.iloc[~X.index.isin(sample),:]\n",
    "\n",
    "    if i < 100:\n",
    "        model1.fit(Xtrain, ytrain)\n",
    "        y_pred.iloc[:,i] = model1.predict(X_test)\n",
    "\n",
    "    elif i < 200:\n",
    "        model2.fit(Xtrain, ytrain)\n",
    "        y_pred.iloc[:,i] = model2.predict(X_test)\n",
    "\n",
    "    elif i < 300:\n",
    "        model3.fit(Xtrain, ytrain)\n",
    "        y_pred.iloc[:,i] = model3.predict(X_test)"
   ]
