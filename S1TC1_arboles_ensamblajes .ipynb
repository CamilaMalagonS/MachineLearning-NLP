   "source": [
    "# Celda 1.1\n",
    "df = pd.DataFrame(bikes.groupby('season').total.mean())\n",
    "df.rename({'total':'promedio'}, axis = 'columns', inplace = True)\n",
    "df.head()\n",
    "\n",
    "df1 = pd.DataFrame(bikes.groupby('season').total.min())\n",
    "df1.rename({'total':'minimo'}, axis = 'columns', inplace = True)\n",
    "df1.head()\n",
    "\n",
    "df2 = pd.DataFrame(bikes.groupby('season').total.quantile(0.25))\n",
    "df2.rename({'total':'percentil 25'}, axis = 'columns', inplace = True)\n",
    "df2.head()\n",
    "\n",
    "df3 = pd.DataFrame(bikes.groupby('season').total.quantile(0.5))\n",
    "df3.rename({'total':'percentil 50'}, axis = 'columns', inplace = True)\n",
    "df3.head()\n",
    "\n",
    "df4 = pd.DataFrame(bikes.groupby('season').total.quantile(0.75))\n",
    "df4.rename({'total':'percentil 75'}, axis = 'columns', inplace = True)\n",
    "df4.head()\n",
    "\n",
    "df5 = pd.DataFrame(bikes.groupby('season').total.max())\n",
    "df5.rename({'total':'maximo'}, axis = 'columns', inplace = True)\n",
    "df5.head()\n",
    "\n",
    "pd.concat([df,df1,df2,df3,df4,df5], axis = 1)"
   ]
